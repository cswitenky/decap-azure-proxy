{
  "platform": {
    "apiRuntime": "node:18"
  },
  "navigationFallback": {
    "rewrite": "/index.html"
  },
  "routes": [
    {
      "route": "/auth",
      "rewrite": "/api/auth"
    },
    {
      "route": "/callback",
      "rewrite": "/api/callback"
    },
    {
      "route": "/api/*",
      "allowedRoles": ["anonymous"]
    }
  ]
}
